<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Disposition_Indicator__c</fullName>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL(Disposition__c , &quot;Substantiated&quot;) , IMAGE(&quot;/resource/AllegationDisposition/substantiated.png&quot;, &quot;Substantiated&quot;),

IF(ISPICKVAL(Disposition__c , &quot;Unsubstantiated&quot;) , IMAGE(&quot;/resource/AllegationDisposition/unsubstantiated.png&quot;, &quot;Unsubstantiated&quot;),

IF(ISPICKVAL(Disposition__c , &quot;Unfounded&quot;) , IMAGE(&quot;/resource/AllegationDisposition/unfounded.png&quot;, &quot;Unfounded&quot;),

IF(ISPICKVAL(Disposition__c, &quot;Null&quot;), null,null))))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Disposition Indicator</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
